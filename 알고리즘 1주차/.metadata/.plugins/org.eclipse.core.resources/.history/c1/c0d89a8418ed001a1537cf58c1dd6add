package ArrayList;

public class ListTest {					// ListTest 클래스
	public static void main(String args[]) {	// main 메소드
		List ArrayList = new List();	// 새로운 List 객체 ArrayList 생성
		int i;
		
		for(i = 1; i <= 6; i++) 
			ArrayList.addLast(i * 10);
		
		ArrayList.showList();
			
		ArrayList.add(3, 11);
		
		ArrayList.showList();
		
		ArrayList.delete(5);

		ArrayList.showList();
	}
}
