package LinkedList;

public class LinkedListTest {					// 연결리스트 테스트
	public static void main(String arg[]) {		// main 메소드
		LinkedList list = new LinkedList();		// 새로운 연결리스트 list 생성 및 초기화
		ListNode temp = new ListNode();
		
		for(int i = 0; i <= 6; i++ ) {
			list.addLastNode(Character.toString((char)(i + 65)));
		}
		
		list.showList();
		
		temp = list.search("A");
		list.insert(temp, "D");
		list.showList();
		
		list.delete("A");
		list.delete("G");
		list.showList();
		
		list.deleteLastNode();
		list.deleteLastNode();
		list.deleteLastNode();
		list.showList();
		
		
	}

}
